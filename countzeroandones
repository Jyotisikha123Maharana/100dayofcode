#include <iostream>
using namespace std ;
 int main (){
     int arr[5 ]= {1, 0 , 0 , 1 , 0 };
     int zeroCount = 0 ; 
     int oneCount = 0 ;
     for ( int i = 0 ; i < 5 ; i ++){
         if ( arr[i]==0){
         zeroCount ++;
         }else {
             oneCount ++;
         }
              cout << zeroCount << endl;
         cout << oneCount << endl;

     }
      cout << zeroCount << endl;
         cout << oneCount << endl;
     
 }
