#include <bits/stdc++.h>
using namespace std;

void problemSolution()
{
  int x, y, a;
  cin >> x >> y >> a;
  if(a >= x && a < y) cout << "YES" << "\n";
  else cout << "NO" << "\n";
}

void testCases()
{
  int tt;
  cin >> tt;
  while(tt--)
  {
    problemSolution();
  }
}

int main()
{
  ios::sync_with_stdio(0);
  cin.tie(0);
  testCases();
  // problemSolution();
  return 0;
}
